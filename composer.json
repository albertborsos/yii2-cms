{
    "name": "albertborsos/yii2-cms",
    "description": "CMS module for Yii2 Framework",
    "keywords": ["yii2", "cms", "extension"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/albertborsos/yii2-cms/issues",
        "source": "https://github.com/albertborsos/yii2-cms"
    },
    "authors": [
        {
            "name": "Albert Borsos",
            "email": "blog@borsosalbert.hu"
        }
    ],
	"respositories":[
		{
			"type":"vcs",
			"url":"git@github.com:albertborsos/yii2-fontawesome.git"
		}
	],
    "require": {
        "albertborsos/yii2-lib": "1.1.*",
        "albertborsos/yii2-tagger": "1.1.*",
        "albertborsos/yii2-user": "1.1.*",
        "albertborsos/yii2-historizer": "1.0.*",
        "rmrevin/yii2-fontawesome": "2.6.2",
        "kartik-v/yii2-nav-x": "1.2.0",
        "kartik-v/yii2-social": "1.2.0"
    },
    "autoload": {
        "psr-4": {
            "albertborsos\\yii2cms\\": ""
        }
    }
}